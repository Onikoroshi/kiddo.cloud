                  %input{:checked => "checked", :name => "optionsCheckboxes", :type => "checkbox"}/
                  Checked
              .checkbox
                %label
                  %input{:disabled => "disabled", :name => "optionsCheckboxes", :type => "checkbox"}/
                  Disabled Unchecked
              .checkbox
                %label
                  %input{:checked => "checked", :disabled => "disabled", :name => "optionsCheckboxes", :type => "checkbox"}/
                  Disabled Checked
            .col-sm-3
              .title
                %h3 Radio Buttons
              .radio
                %label
                  %input{:name => "optionsRadios", :type => "radio"}/
                  Radio is off
              .radio
                %label
                  %input{:checked => "true", :name => "optionsRadios", :type => "radio"}/
                  Radio is on
              .radio
                %label
                  %input{:disabled => "disabled", :name => "optionsRadiosDisabled", :type => "radio"}/
                  Disabled Radio is off
              .radio
                %label
                  %input{:checked => "true", :disabled => "disabled", :name => "optionsRadiosDisabled", :type => "radio"}/
                  Disabled Radio is on
            .col-sm-3
              .title
                %h3 Toggle Buttons
              .togglebutton
                %label
                  %input{:checked => "checked", :type => "checkbox"}/
                  Toggle is on
              .togglebutton
                %label
                  %input{:type => "checkbox"}/
                  Toggle is off
