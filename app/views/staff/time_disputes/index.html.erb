<p id="notice"><%= notice %></p>

<h1>Time Disputes</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Date</th>
      <th>Location</th>
      <th>Message</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_disputes.each do |time_dispute| %>
      <tr>
        <td><%= time_dispute.first_name %></td>
        <td><%= time_dispute.last_name %></td>
        <td><%= time_dispute.email %></td>
        <td><%= time_dispute.phone %></td>
        <td><%= time_dispute.date %></td>
        <td><%= time_dispute.location %></td>
        <td><%= time_dispute.message %></td>
        <td><%= link_to 'Show', time_dispute %></td>
        <td><%= link_to 'Edit', edit_time_dispute_path(time_dispute) %></td>
        <td><%= link_to 'Destroy', time_dispute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Dispute', new_time_dispute_path %>
